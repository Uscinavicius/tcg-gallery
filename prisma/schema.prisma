generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Card {
  id                    String @id @default(uuid())
  name                  String
  rarity                String
  price                 Float @default(0.0)
  reverseHoloAvg1       Float @default(0.0)
  imageUrl              String
}
